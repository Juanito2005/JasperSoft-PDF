	<parameter name="pFechaFin" class="java.sql.Date"/>
	<queryString>
		<![CDATA[SELECT p.id as pedido_id, c.nombre as cliente, p.fecha, p.total
FROM pedidos p
JOIN clientes c ON p.id_cliente = c.id
WHERE p.fecha >= $P{pFechaInicio} 
  AND p.fecha <= $P{pFechaFin}
  AND c.id = $P{pClienteId}]]>
	</queryString>
